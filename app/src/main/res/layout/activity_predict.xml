    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.dmitry.diplom_averin.view.activity.PredictActivity">

    <ImageView
        android:id="@+id/activity_predict_image_view"
